{
  "name": "@sigmasms/otp-sdk-examples",
  "version": "1.0.0",
  "description": "Sigma OTP SDK client with examples",
  "main": "./dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild": "rm -rf ./dist",
    "build": "tsc",

    "examples:use-express-controller": "tsc-watch src/examples/use-express-controller.ts --outDir ./dist --onSuccess \"node dist/examples/use-express-controller.js\"",
    "examples:use-fastify-controller": "tsc-watch src/examples/use-fastify-controller.ts --outDir ./dist --onSuccess \"node dist/examples/use-fastify-controller.js\"",
    "examples:use-bunjs-controller": "bun src/examples/use-bunjs-controller.ts",
    "examples:use-nestjs-controller": "tsc-watch -p ./tsconfig-nestjs.json --outDir ./dist --onSuccess \"node dist/examples/use-nestjs-controller.js\"",
    "examples:test-controller": "tsc-watch src/tests/test-controller.ts --outDir ./dist --onSuccess \"node dist/test-controller.js\"",
    
    "examples:full": "tsc-watch src/examples/full.ts --outDir ./dist --onSuccess \"node dist/full.js\"",
    "examples:full:js": "node src/examples/full.js",
    "examples:full-callbacks": "tsc-watch src/examples/full-callbacks.ts --outDir ./dist --onSuccess \"node dist/examples/full-callbacks.js\"",
    "examples:full-callbacks-js": "node src/examples/full-callbacks.js",

    "examples:simple-code": "tsc-watch src/examples/simple-code.ts --outDir ./dist --onSuccess \"node dist/examples/simple-code.js\"",
    "examples:simple-codeless": "tsc-watch src/examples/simple-codeless.ts --outDir ./dist --onSuccess \"node dist/examples/simple-codeless.js\"",
    "examples:events": "tsc-watch src/examples/events.ts --outDir ./dist --onSuccess \"node dist/examples/events.js\""
    
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sigmasms/otp-sdk-examples.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sigmasms/otp-sdk-examples/-/issues"
  },
  "homepage": "https://github.com/sigmasms/otp-sdk-examples/-/blob/master/README.md",
  "devDependencies": {
    "@golevelup/ts-jest": "^0.3.1",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/gitlab": "^9.4.2",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.17",
    "@types/supertest": "^2.0.11",
    "conventional-changelog-conventionalcommits": "^5.0.0",
    "jest": "^29.3.1",
    "jest-extended": "^3.2.0",
    "reflect-metadata": "^0.1.13",
    "semantic-release": "^19.0.5",
    "ts-jest": "^29.0.3",
    "ts-node": "~10.9.1",
    "tsc-watch": "^6.0.4",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@nestjs/common": "^10.3.7",
    "@nestjs/core": "^10.3.7",
    "@nestjs/platform-express": "^10.3.7",
    "@sigmasms/otp-sdk": "1.0.4",
    "express": "^4.19.2",
    "fastify": "^4.26.2",
    "fastify-print-routes": "^3.2.0"
  }
}
